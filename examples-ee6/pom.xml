                           <phase>post-integration-test</phase>
                           <configuration>
                              <tasks>
                                 <delete failonerror="false">
                                    <fileset dir="${jboss.master.configuration}/farm">
                                       <include name="jboss-seam-${example.name}-ds.xml"/>
                                       <include name="${example.context.path}.ear"/>
                                    </fileset>
                                 </delete>
                              </tasks>
                           </configuration>
                           <goals>
                              <goal>run</goal>
                           </goals>
                        </execution>
                     </executions>
                  </plugin>
               </plugins>
            </pluginManagement>
         </build>
      </profile>
   </profiles>

